void setup()
{
 size(100,100); 
}

void draw()
{
  // clear screen to white
  background(255,255,255);
  // set colour to red
  stroke(255,0,0);
  // draw point
  point(0,0);
  // set colour to green
  stroke(0,255,0);
  // draw point
  point(1,0);
  // set colour to blue
  stroke(0,0,255);
  // draw point
  point(2,0);

}
